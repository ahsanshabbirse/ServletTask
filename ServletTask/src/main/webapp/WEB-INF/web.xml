<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetTaskController</servlet-name>
  	<display-name>GetTaskController</display-name>
  	<description></description>
  	<servlet-class>com.test.web.GetTaskController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SetTaskController</servlet-name>
  	<display-name>SetTaskController</display-name>
  	<description></description>
  	<servlet-class>com.test.web.SetTaskController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDelTaskController</servlet-name>
  	<display-name>GetDelTaskController</display-name>
  	<description></description>
  	<servlet-class>com.test.web.GetDelTaskController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SetModTaskController</servlet-name>
  	<display-name>SetModTaskController</display-name>
  	<description></description>
  	<servlet-class>com.test.web.SetModTaskController</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>GetTaskController</servlet-name>
  	<url-pattern>/getTask</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SetTaskController</servlet-name>
  	<url-pattern>/setTask</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDelTaskController</servlet-name>
  	<url-pattern>/getDelTask</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SetModTaskController</servlet-name>
  	<url-pattern>/setModTask</url-pattern>
  </servlet-mapping>
</web-app>
